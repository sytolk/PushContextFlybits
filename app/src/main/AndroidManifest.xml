<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bosch.mx.lud1ga.pushcontextflybits270">

    <!-- Permissions -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="bosch.mx.lud1ga.pushcontextflybits270.permission.C2D_MESSAGE" />
    <permission android:name="bosch.mx.lud1ga.pushcontextflybits270.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="android.permission.INTERNET" />


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- BEACON CONTEXT PERMISSION -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- BEACON CONTEXT PERMISSION -->
    <permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- BEACON CONTEXT PERMISSION -->
    <permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- BEACON CONTEXT PERMISSION -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="bosch.mx.lud1ga.pushcontextflybits270.PushContextApplication">

        <activity android:name="bosch.mx.lud1ga.pushcontextflybits270.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data android:name="com.flybits.apikey"
            android:value="DC8DB88D-7FC3-4CF9-B3F6-C696179AE957" />

        <service
            android:name="com.flybits.core.api.services.gcm.FlybitsGCMTokenListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>
        <service android:name="com.flybits.core.api.services.gcm.FlybitsGCMRegistrationIntentService"/>

        <service
            android:name="bosch.mx.lud1ga.pushcontextflybits270.services.FlybitsGCMListener"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="bosch.mx.lud1ga.pushcontextflybits270" />
            </intent-filter>
        </receiver>

        <!--<service android:name="com.flybits.core.api.context.v2.plugins.network.NetworkBackgroundService"-->
        <service android:name="com.flybits.core.api.context.v2.plugins.network.NetworkContextPluginService"
            android:permission="com.google.android.gms.permission.BIND_NETWORK_TASK_SERVICE"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.android.gms.gcm.ACTION_TASK_READY"/>
            </intent-filter>
        </service>

        <!--<service android:name="com.flybits.core.api.context.v2.plugins.battery.BatteryBackgroundService"-->
        <service android:name="com.flybits.core.api.context.v2.plugins.battery.BatteryContextPluginService"
            android:permission="com.google.android.gms.permission.BIND_NETWORK_TASK_SERVICE"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.android.gms.gcm.ACTION_TASK_READY"/>
            </intent-filter>
        </service>
        <uses-permission android:name="android.permission.BLUETOOTH"/> <!-- BEACON CONTEXT PERMISSION -->


        <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/> <!-- BEACON CONTEXT PERMISSION -->

        <!--<service android:name="com.flybits.core.api.context.v2.plugins.location.LocationBackgroundService"-->
        <service android:name="com.flybits.core.api.context.v2.plugins.location.LocationContextPluginService"
            android:permission="com.google.android.gms.permission.BIND_NETWORK_TASK_SERVICE"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.android.gms.gcm.ACTION_TASK_READY"/>
            </intent-filter>
        </service>

        <service android:name="com.flybits.core.api.context.v2.services.ContextRulesService"
            android:permission="com.google.android.gms.permission.BIND_NETWORK_TASK_SERVICE"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.android.gms.gcm.ACTION_TASK_READY"/>
            </intent-filter>
        </service>
        <service android:name="com.flybits.core.api.context.v2.services.ContextUploadingService"
            android:permission="com.google.android.gms.permission.BIND_NETWORK_TASK_SERVICE"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.android.gms.gcm.ACTION_TASK_READY"/>
            </intent-filter>

        </service>

        <!--<service android:name="com.flybits.core.api.context.v2.plugins.beacon.BeaconContextBackgroundService"-->
        <!--android:permission="com.google.android.gms.permission.BIND_NETWORK_TASK_SERVICE"-->
        <!--android:exported="true">-->
        <!--<intent-filter>-->
        <!--<action android:name="com.google.android.gms.gcm.ACTION_TASK_READY"/>-->
        <!--</intent-filter>-->
        <!--</service>-->

        <service android:name="com.flybits.core.api.context.v2.plugins.beacon.BeaconScanningService"
            android:enabled="true"/>

        <service android:name="com.flybits.core.api.services.PushService" />
    </application>

</manifest>